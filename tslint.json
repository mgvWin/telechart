{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": false,
        "arrow-parens": false,
        "quotemark": {
            "options": "single"
        },
        "align": false,
        "ordered-imports": false,
        "no-console": false,
        "object-literal-sort-keys": false,
        "max-line-length": false
    },
    "rulesDirectory": []
}